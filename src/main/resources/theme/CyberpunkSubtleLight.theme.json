        "selectedForeground": "selectionForeground",
        "hoverMaskColor": "accentColorTransparent",
        "underlineGlow": {
          "color": "glowColor",
          "offset": "0,0",
          "spread": "2",
          "size": "3"
        }
      },
      "Button": {
        "selectedBackground": "selectionBackground",
        "selectedForeground": "selectionForeground",
        "hoverBackground": "secondaryBackground",
        "hoverForeground": "primaryForeground",
        "selectedBorderColor": "accentColor",
        "hoverBorderColor": "accentColorTransparent"
