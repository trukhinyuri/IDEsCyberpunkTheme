      "endBorderColor": "neonBorderColor",
      "focusedBorderColor": "focusBorderColor",
      "shadowColor": "neonShadowColor",
      "shadowWidth": 3,
      "default": {
        "foreground": "#FFFFFF",
        "startBackground": "accentColor",
        "endBackground": "accentColor",
        "startBorderColor": "accentColor",
        "endBorderColor": "accentColor",
        "focusedBorderColor": "highlightColor",
        "shadowColor": "neonShadowColor",
        "shadowWidth": 4
      }
